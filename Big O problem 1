/*Javascript*/

function funChallenge(input){
  let a = 10; //0(1)
  a = 50 + 3; //0(1)
  
  for(let i = 0; i < input.length; i++){ //0(n)
    anotherFunction(); //0(n)
    let stranger = true; //0(n)
    a++; //0(n)
  }
  return a; //0(1)
}
 BIG 0(3 + 4n) ==> BIG 0(n)
 
 
/*Java*/

public int funChallenge(int [] input) {
  int a = 10;
  a = 50 + 3;
  
  for(int i = 0; i < input.length; i++){
    anotherFunction();
    let stranger = true;
    a++;
  }
  
  return a;
}
